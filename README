## Primer proyecto

function main() {
    var distance = parseInt(readLine(), 10);
    //tu código va aquí
    let resultado = distance /40 *60;
    console . log("", resultado );
}


## Segundo proyecto

function main() {
    var depth = parseInt(readLine(), 10);
    //tu código va aquí
    
    var dia = 0;
    var total = 0;
    while(total<depth){
        dia = dia + 1;
        total = total + 7;
        if(total >=depth){
            console.log(dia);
            break;
        }
        total = total - 2;
    }
}

Tercer proyecto

function main() {
    var cantidad = parseFloat(readLine(), 10);
    var tasa = parseFloat(readLine(), 10);
    
    console.log(convertir(cantidad, tasa));
}
function convertir (cantidad,tasa){
	return cantidad * tasa 
}

Cuarto proyecto

function contact(name, number) {
    this.name = name;
    this.number = number;
    this.print = () =>{
    console.log(this.name+": "+this.number);
  }
}

var a = new contact("David", 12345);
var b = new contact("Amy", 987654321)
a.print();
b.print();

Quinto proyecto


function main() {
    var aumentar = parseInt(readLine(), 10);
    var precio = [98.99, 15.2, 20, 1026];
    //your code goes here
    for (i = 0; i < precio.length; i++) {
        precio[i] += aumentar;
    }

    console.log(precio);
}

Sexto proyecto

class Add {
  constructor(...words) {
      this.words = words;
  }
  //your code goes here
  print(){
      let z="";
      for(x of this.words){
           z=z+"$"+x;
      }
      console.log(z + "$");
      
  }
}


var x = new Add("hehe", "hoho", "haha", "hihi", "huhu");
var y = new Add("this", "is", "awesome");
var z = new Add("lorem", "ipsum", "dolor", "sit", "amet", "consectetur", "adipiscing", "elit");
x.print();
y.print();
z.print();